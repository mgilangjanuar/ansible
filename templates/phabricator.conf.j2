<VirtualHost *:999>
    ServerAdmin webmaster@localhost
    ServerName _
    DocumentRoot /var/www/app/phabricator/webroot
    RewriteEngine on
    RewriteRule ^/rsrc/(.*)     -                       [L,QSA]
    RewriteRule ^/favicon.ico   -                       [L,QSA]
    RewriteRule ^(.*)$          /index.php?__path__=$1  [B,L,QSA]
    <Directory "/var/www/app/phabricator/webroot">
        Order allow,deny
        Allow from all
    </Directory>
</VirtualHost>